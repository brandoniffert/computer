{
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2020
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jquery": true
  },
  "ignorePatterns": [
    "node_modules",
    "dist",
    "**/*.min.js"
  ],
  "rules": {
    "no-console": 1
  }
}
